<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="433.0" prefWidth="744.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.news_recommendation.WebViewController">
   <children>
      <WebView fx:id="webView" layoutY="7.0" prefHeight="423.0" prefWidth="605.0" />
      <Button fx:id="likebtn" layoutX="636.0" layoutY="30.0" mnemonicParsing="false" onAction="#likehandle" prefHeight="38.0" prefWidth="78.0" text="Like" />
      <Button fx:id="dislikebtn" layoutX="636.0" layoutY="87.0" mnemonicParsing="false" onAction="#dislikehandle" text="Dislike" />
      <Button fx:id="saveabtn" layoutX="635.0" layoutY="147.0" mnemonicParsing="false" onAction="#SavedArticles" prefHeight="38.0" prefWidth="78.0" text="Save" />
      <ComboBox fx:id="ratecombobox" layoutX="632.0" layoutY="340.0" prefHeight="26.0" prefWidth="82.0" promptText="Rate" />
      <Button fx:id="ratesubmitbtn" layoutX="633.0" layoutY="379.0" mnemonicParsing="false" onAction="#ratehandle" text="Submit" />
   </children>
</AnchorPane>
